<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Scene fx:id="scene" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChangeController">
    <Pane fx:id="pane" prefHeight="180.0" prefWidth="300.0">
      <children>
         <TextField fx:id="target" layoutX="34.0" layoutY="25.0" prefHeight="25.0" prefWidth="230.0" promptText="What you want to change">
            <font>
               <Font size="16.0" />
            </font></TextField>
         <TextField fx:id="newInfo" layoutX="34.0" layoutY="72.0" prefHeight="36.0" prefWidth="230.0" promptText="New Info">
            <font>
               <Font size="16.0" />
            </font></TextField>
         <Button fx:id="ChangeButton" layoutX="34.0" layoutY="125.0" mnemonicParsing="false" onAction="#Change" prefHeight="30.0" prefWidth="110.0" text="Change">
            <font>
               <Font name="System Bold" size="14.0" />
            </font>
         </Button>
         <Button fx:id="ExitButton" layoutX="174.0" layoutY="125.0" mnemonicParsing="false" onAction="#Exit" prefHeight="30.0" prefWidth="90.0" text="Exit">
            <font>
               <Font name="System Bold" size="14.0" />
            </font>
         </Button>
      </children></Pane>
</Scene>
